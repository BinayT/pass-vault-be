{
  "name": "pass-vault",
  "version": "1.0.0",
  "description": "Pass valut application's backend where it will serve a frontend React Native application.",
  "main": "src/index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc"
  },
  "keywords": [
    "node",
    "express",
    "api",
    "backend",
    "typescript"
  ],
  "_moduleAliases": {
    "@models": "dist/models",
    "@controllers": "dist/controllers",
    "@config": "dist/config",
    "@routes": "dist/routes",
    "@helpers": "dist/helpers",
    "@utils": "dist/utils"
  },
  "author": "bindev",
  "type": "commonjs",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "argon2": "^0.41.1",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "pg": "^8.13.0",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.4",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
